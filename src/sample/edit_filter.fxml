<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EditFilterController">
   <children>
      <ComboBox fx:id="filterbox" layoutX="225.0" layoutY="53.0" onAction="#initializeCurrentList" prefWidth="150.0" />
      <ListView fx:id="currentList" layoutX="35.0" layoutY="146.0" prefHeight="173.0" prefWidth="200.0" />
      <ListView fx:id="allFoodList" layoutX="359.0" layoutY="146.0" prefHeight="173.0" prefWidth="200.0" />
      <Label layoutX="225.0" layoutY="25.0" text="Filter:" />
      <Label layoutX="35.0" layoutY="109.0" text="Current food list:" />
      <Button layoutX="45.0" layoutY="326.0" mnemonicParsing="false" onAction="#deleteFoodItem" text="DELETE" />
      <Button layoutX="427.0" layoutY="326.0" mnemonicParsing="false" onAction="#addItem" text="ADD" />
      <Button layoutX="146.0" layoutY="326.0" mnemonicParsing="false" onAction="#resetList" text="RESET" />
      <Button layoutX="268.0" layoutY="385.0" mnemonicParsing="false" onAction="#confirmationExecute" text="CONFIRM" />
      <Label layoutX="358.0" layoutY="109.0" text="Add Food List:" />
   </children>
</AnchorPane>
